  "shortcuts": [
    {
      "name": "Urgence Juridique",
      "short_name": "Urgence",
      "description": "Accès rapide à l'assistance juridique d'urgence",
      "url": "/urgence",
      "icons": [
        {
          "src": "/icons/shortcut-urgence.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Consultation",
      "short_name": "Conseil",
      "description": "Demander une consultation juridique",
      "url": "/consultation",
      "icons": [
        {
          "src": "/icons/shortcut-consultation.png",
          "sizes": "96x96", 
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Documents",
      "short_name": "Docs",
      "description": "Accéder aux documents et guides",
      "url": "/documents",
