  "scripts": {
    "predev": "node scripts/gen-firebase-config.cjs",
    "dev": "vite",
    "prebuild": "node scripts/gen-firebase-config.cjs && node scripts/download-sitemaps.js",
    "build": "tsc -b && vite build && node scripts/copy-sitemaps-to-dist.js",
    "preview": "vite preview --strictPort",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "analyze": "vite build --mode analyze",
    "gen-env": "node scripts/generate-env-files.cjs",
    "build:force": "vite build",
    "download-sitemaps": "node scripts/download-sitemaps.js",
    "build:with-sitemaps": "npm run download-sitemaps && npm run build",
    "postbuild": "node scripts/copy-sitemaps-to-dist.js && react-snap",
    "dev:emulators": "firebase emulators:start --only firestore,functions,auth,storage",
    "dev:functions": "cd firebase/functions && npm run build:watch",
    "kill-emulators": "node scripts/kill-emulators.cjs",
    "dev:full": "node scripts/check-emulator-env.cjs && node scripts/check-emulator-ports.cjs && concurrently -n \"EMULATORS,FUNCTIONS,FRONTEND\" -c \"blue,green,yellow\" --kill-others-on-fail \"npm run dev:emulators\" \"npm run dev:functions\" \"npm run dev\"",
    "clean:tmp": "node -e \"const fs = require('fs'); const path = require('path'); const cleanup = (dir) => { const items = fs.readdirSync(dir).filter(f => f.startsWith('tmpclaude-')); items.forEach(f => { const p = path.join(dir, f); const s = fs.statSync(p); if (s.isDirectory()) fs.rmSync(p, {recursive:true}); else fs.unlinkSync(p); }); return items.length; }; const root = path.join(__dirname, '..'); let total = cleanup(root); const subdirs = ['sos', 'sos/firebase/functions', 'sos/firebase/functions/src', 'sos/cloudflare-worker', 'sos/src/locales', 'sos/src/locales/fr-fr', 'sos/src/locales/en']; subdirs.forEach(d => { try { total += cleanup(path.join(root, d)); } catch {} }); console.log('Cleaned ' + total + ' temp files');\"",
    "clean:tmp:recursive": "node -e \"const fs = require('fs'); const path = require('path'); let count = 0; const walk = (dir) => { try { fs.readdirSync(dir).forEach(f => { const p = path.join(dir, f); const s = fs.statSync(p); if (s.isDirectory() && !f.includes('node_modules')) walk(p); if (f.startsWith('tmpclaude-')) { if (s.isDirectory()) fs.rmSync(p, {recursive:true}); else fs.unlinkSync(p); count++; } }); } catch {} }; walk(path.join(__dirname, '..')); console.log('Cleaned ' + count + ' temp files recursively');\""
  },
